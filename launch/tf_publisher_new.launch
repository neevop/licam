<?xml version="1.0"?>     
<launch>

<!-- A simple launch file for the urg_node package. -->

  <arg name="agent_serial" default="$(env HOSTNAME)"/>

  <!-- <node pkg="tf" type="static_transform_publisher" name="base_imu" args="0 0 0 0 0 0 $(arg agent_serial)/imu_link imu_link 100"/> -->
  <!-- <node pkg="tf" type="static_transform_publisher" name="base_vlp" args="0 0 0 0 0 0 $(arg agent_serial)/horizontal_velodyne_link horizontal_velodyne_link 100"/> -->
  <!-- <node pkg="tf" type="static_transform_publisher" name="base_foot" args="0 0 0 0 0 0 $(arg agent_serial)/base_link base_link 100"/> -->

  <param name="robot_description" textfile="$(find licam)/urdf/sensor_pod.urdf" />

  <node name="robot_state_publisher" pkg="robot_state_publisher" type="robot_state_publisher">
    <param name="publish_frequency" type="double" value="10.0"/>
    <param name="use_tf_static" type="bool" value="false"/>
    <!-- <param name="tf_prefix" type="string" value="$(arg agent_serial)"/> -->
    <remap from="joint_states" to="different_joint_states" /> 
  </node>

</launch>
